{
  "name": "vite-project",
  "displayName": "display name",
  "description": "description",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env IS_CRXJS=true rimraf ./dist && vite --config build-tools/vite-config/vite.config.crxjs.ts",
    "build": "tsc && vite build --config build-tools/vite-config/vite.config.crxjs.ts",
    "preview": "vite preview",
    "dev:prepare:chrome": "cross-env NODE_ENV=\"development\" npm run dev:chrome ",
    "build:prepare": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" NODE_ENV=\"production\" npm run webpack:build",
    "build:prepare:chrome": "cross-env NODE_ENV=\"production\" npm run build:chrome",
    "webpack:build": "yarn run build:chrome && yarn run build:firefox && yarn run build:opera",
    "build:chrome": "cross-env TARGET_BROWSER=\"chrome\" webpack --config build-tools/webpack-config/webpack.config.ts && cross-env TARGET_BROWSER=\"chrome\" npm run write:manifest",
    "dev:chrome1": "cross-env TARGET_BROWSER=\"chrome\" webpack --config build-tools/webpack-config/webpack.config.ts --watch && cross-env TARGET_BROWSER=\"chrome\" npm run write:manifest",
    "dev:chrome": "cross-env TARGET_BROWSER=\"chrome\" npm run write:manifest && cross-env TARGET_BROWSER=\"chrome\" webpack --config build-tools/webpack-config/webpack.config.ts --watch",
    "build:firefox": "cross-env TARGET_BROWSER=\"firefox\" webpack --config build-tools/webpack-config/webpack.config.ts && cross-env TARGET_BROWSER=\"chrome\" npm run write:manifest",
    "build:opera": "cross-env TARGET_BROWSER=\"opera\" webpack --config build-tools/webpack-config/webpack.config.ts && cross-env TARGET_BROWSER=\"chrome\" npm run write:manifest",
    "write:manifest": "esno build-tools/dev-scripts/manifest.ts",
    "clear": "rimraf  extension/dist extension/manifest.json extension/.*"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^7.5.5",
    "styled-components": "^5.3.5",
    "webextension-polyfill": "^0.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@crxjs/vite-plugin": "^1.0.14",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/chrome": "^0.0.181",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@types/webextension-polyfill": "^0.9.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js-pure": "^3.25.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "esbuild-loader": "^2.19.0",
    "esno": "^0.16.3",
    "filemanager-webpack-plugin": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "kolorist": "^1.5.1",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "path": "^0.12.7",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "react-refresh": "^0.14.0",
    "resolve-typescript-plugin": "^1.2.0",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.9.2",
    "vite": "^3.1.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-node-externals": "^3.0.0",
    "wext-manifest-webpack-plugin": "^1.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  }
}
